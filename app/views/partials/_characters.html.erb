<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Character</th>
      <th>Pronounciation</th>
      <th>Part of Speech</th>
      <th>Meaning</th>
    </tr>
  </thead>
  <tbody>
    <% flash_cards.each do |fc| %>
      <tr>
        <td><%= link_to fc.send(@charset), polymorphic_url([fc.teachable, fc]) %></td>
        <%# <td><%= fc.send @charset %1></td> %>
        <td><%= fc.send @transcript %></td>
        <td><%= fc.part_of_speech %></td>
        <td><%= fc.meaning %></td>
      </tr>
    <% end %>
  </tbody>
</table>
